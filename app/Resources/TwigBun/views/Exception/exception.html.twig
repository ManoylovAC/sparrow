{% extends '::front.html.twig' %}
{% block title %}
    Oups!
{% endblock %}
{% block body %}
<div class="innerAll">
    <div class="error innerAll ">

        <div class="well text-center">
            <div class="center text-large innerAll">
                <i class="fa fa-5x fa-chain-broken text-danger "></i>
            </div>
            <h1 class="strong innerTB">Oups!</h1>
            <h4 class="innerB">Something went wrong.</h4>
            <div class="well bg-white text-danger strong ">
                {% if  status_code == 404 %} <br> This page is does not exist!
                    {{ status_code }} {{ status_text }}
                {% elseif status_code == 403 %}
                    {{ status_code }} {{ status_text }} <br> You went the wrong way!
                {% endif %}
            </div>
        </div>
    </div>
</div>
    {% endblock %}